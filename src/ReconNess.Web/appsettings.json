{
  "ReconNess": {
    "Version" :  "1.6.3"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=postgres;Database={{database}};Username={{username}};Password={{password}};Persist Security Info=True",
    "DefaultRabbitmqConnection": "amqp://{{username}}:{{password}}@rabbitmq:5672"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtIssuerOptions": {
    "Issuer": "reconness",
    "Audience": "http://www.reconness.com/"
  }
}
